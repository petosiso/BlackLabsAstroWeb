{
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/index.php",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/www.blacklabs.sk/*",
      "redirect": "https://blacklabs.sk/{*}",
      "statusCode": 301
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  }
}